{
    "name": "mateusjunges/laravel-acl",
    "description": "This package provides a complete Access Control List management",
    "type": "laravel-package",
    "keywords": [
        "laravel",
        "junges",
        "acl",
        "security",
        "permissions",
        "access-control-list"
    ],
    "homepage": "https://github.com/mateusjunges/laravel-acl",
    "require": {
        "php": ">=7.0",
        "illuminate/auth": "~5.4.*|~5.5.*|~5.6.*|~5.7.*|~5.8.*|~6.0",
        "illuminate/database": "~5.4.*|~5.5.*|~5.6.*|~5.7.*|~5.8.*|~6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4.2|~3.5.0|~3.6.0|~3.7.0",
        "phpunit/phpunit": "^5.7|6.2|^7.0",
        "predis/predis": "^1.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mateus Junges",
            "email": "contato@mateusjunges.com",
            "homepage": "https://mateusjunges.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Junges\\ACL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Junges\\ACL\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Junges\\ACL\\ACLServiceProvider",
                "Junges\\ACL\\ACLAuthServiceProvider",
                "Junges\\ACL\\ACLEventsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
